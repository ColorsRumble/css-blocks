{
  "name": "@css-blocks/website",
  "version": "0.21.0",
  "private": true,
  "dependencies": {
    "@css-blocks/jsx": "^0.21.0",
    "@css-blocks/runtime": "^0.21.0",
    "@css-blocks/webpack": "^0.21.0",
    "@types/jest": "^24.0.11",
    "@types/react-router-dom": "^4.2.3",
    "autoprefixer": "9.5.1",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^8.0.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "css-loader": "2.1.1",
    "dotenv": "7.0.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.6.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.7.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.3",
    "raf": "3.4.1",
    "react": "^16.1.1",
    "react-dev-utils": "^9.0.0",
    "react-dom": "^16.1.2",
    "react-ga": "^2.5.0",
    "react-router-dom": "^5.0.0",
    "snyk": "^1.116.2",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-loader": "^5.4.3",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-dev-server": "3.3.1",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-sources": "^1.0.2",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "compile": "yarn run build",
    "test": "yarn run test:runner",
    "serve": "node server.js",
    "snyk-protect": "snyk protect",
    "test:runner": "echo 'nothing to test'",
    "prepublish": "npm run snyk-protect"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "../../../node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "clean-css": "^4.1.11",
    "obj-str": "^1.0.0"
  },
  "snyk": true
}