{
  "private": true,
  "name": "ember",
  "publisher": "css-blocks",
  "description": "VSCode Extension for CSS Blocks + Ember",
  "author": "LinkedIn",
  "license": "BSD-2-Clause",
  "version": "1.0.0-alpha.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedIn/css-blocks"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "activationEvents": [
    "onLanguage:handlebars",
    "onLanguage:scss",
    "onLanguage:css"
  ],
  "contributes": {
    "css": {
      "customData": [
        "./css-blocks.css-data.json"
      ]
    }
  },
  "main": "./dist/extension",
  "scripts": {
    "vscode:prepublish": "tsc --build",
    "update-vscode": "vscode-install",
    "postinstall": "vscode-install",
    "compile": "tsc --build",
    "watch": "tsc --build --watch",
    "package": "vsce package"
  },
  "dependencies": {
    "@css-blocks/language-server": "^1.0.0-alpha.0",
    "vscode-languageclient": "^5.2.1"
  },
  "devDependencies": {
    "vsce": "^1.69.0",
    "vscode": "^1.1.35"
  }
}
